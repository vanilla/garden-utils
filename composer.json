    "type": "library",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/polyfill-php80": "^1.27"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^5.14"
    },
    "license": "MIT",
    "autoload": {
